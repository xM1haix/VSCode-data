{
    "3dviewer.background": "#000000",
    "[dart]": {
        "editor.defaultFormatter": "Dart-Code.dart-code",
        "editor.detectIndentation": false,
        "editor.inlayHints.enabled": "on",
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "insert",
        "editor.tabSize": 2
    },
    "better-comments.tags": [
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#FF0000",
            "italic": false,
            "strikethrough": false,
            "tag": "!",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#0000FF",
            "italic": false,
            "strikethrough": false,
            "tag": "?",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#474747",
            "italic": false,
            "strikethrough": true,
            "tag": "//",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#FF8C00",
            "italic": false,
            "strikethrough": false,
            "tag": "todo",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#00FF00",
            "italic": false,
            "strikethrough": false,
            "tag": "*",
            "underline": false
        },
        {
            "backgroundColor": "transparent",
            "bold": false,
            "color": "#FFFFFF",
            "italic": false,
            "strikethrough": false,
            "tag": "-",
            "underline": false
        }
    ],
    "breadcrumbs.enabled": false,
    "breadcrumbs.filePath": "off",
    "dart.devToolsBrowser": "default",
    "dart.devToolsLocation": {
        "cpu-profiler": "external",
        "default": "beside",
        "inspector": "external",
        "logging": "external",
        "memory": "external",
        "network": "external",
        "performance": "external"
    },
    "dart.documentation": "full",
    "dart.flutterGenerateLocalizationsOnSave": "allIfDirty",
    "dart.flutterGutterIcons": true,
    "dart.flutterHotReloadOnSave": "allIfDirty",
    "dart.flutterOutline": true,
    "dart.flutterRememberSelectedDevice": true,
    "dart.flutterScreenshotPath": null,
    "dart.flutterSdkPath": "C:\\Flutter\\flutter",
    "dart.flutterSelectDeviceWhenConnected": true,
    "dart.flutterShowEmulators": "always",
    "dart.flutterShowWebServerDevice": "always",
    "dart.flutterWebRenderer": "auto",
    "dart.hotReloadOnSave": "allIfDirty",
    "dart.hotReloadProgress": "statusBar",
    "dart.lineLength": 80,
    "dart.lspSnippetTextEdits": true,
    "dart.maxCompletionItems": null,
    "dart.maxLogLineLength": 10000,
    "dart.normalizeFileCasing": true,
    "dart.onlyAnalyzeProjectsWithOpenFiles": true,
    "dart.previewCommitCharacters": false,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "dart.previewHotReloadOnSaveWatcher": true,
    "dart.projectSearchDepth": 5,
    "dart.renameFilesWithClasses": "always",
    "dart.sdkPath": "C:\\Flutter\\bin",
    "editor.codeActionsOnSave": {
        "source": "always",
        "source.fixAll": "always",
        "source.organizeImports": "always",
        "source.sortMembers": "always"
    },
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.hover.delay": 0,
    "editor.minimap.enabled": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.renderWhitespace": "none",
    "editor.selectionHighlight": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 8,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.wordBasedSuggestions": "allDocuments",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 1,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "files.autoSave": "onWindowChange",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "search.followSymlinks": false,
    "security.workspace.trust.banner": "always",
    "security.workspace.trust.startupPrompt": "always",
    "security.workspace.trust.untrustedFiles": "open",
    "window.restoreFullscreen": true,
    "window.zoomLevel": 1,
    "window.zoomPerWindow": false,
    "workbench.colorCustomizations": {
        "editor.background": "#000000"
    },
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.startupEditor": "none",
    "workbench.trustedDomains.promptInTrustedWorkspace": true
}
